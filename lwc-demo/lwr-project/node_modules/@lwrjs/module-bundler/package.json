{
    "name": "@lwrjs/module-bundler",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.13.3",
    "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce-experience-platform-emu/lwr.git",
        "directory": "packages/@lwrjs/module-registry"
    },
    "bugs": {
        "url": "https://github.com/salesforce-experience-platform-emu/lwr/issues"
    },
    "type": "module",
    "types": "build/es/index.d.ts",
    "main": "build/cjs/index.cjs",
    "module": "build/es/index.js",
    "exports": {
        ".": {
            "import": "./build/es/index.js",
            "require": "./build/cjs/index.cjs"
        },
        "./amd-bundle-provider": {
            "import": "./build/es/amd-bundle-provider.js",
            "require": "./build/cjs/amd-bundle-provider.cjs"
        },
        "./amd-runtime-bundle-provider": {
            "import": "./build/es/amd-runtime-bundle-provider.js",
            "require": "./build/cjs/amd-runtime-bundle-provider.cjs"
        },
        "./esm-bundle-provider": {
            "import": "./build/es/esm-bundle-provider.js",
            "require": "./build/cjs/esm-bundle-provider.cjs"
        }
    },
    "scripts": {
        "build": "tsc -b",
        "clean": "rimraf build",
        "test": "jest"
    },
    "files": [
        "build/**/*.js",
        "build/**/*.cjs",
        "build/**/*.d.ts"
    ],
    "dependencies": {
        "@lwrjs/diagnostics": "0.13.3",
        "@lwrjs/instrumentation": "0.13.3",
        "@lwrjs/shared-utils": "0.13.3",
        "@rollup/plugin-replace": "^5.0.7",
        "rollup": "^2.78.0"
    },
    "devDependencies": {
        "@lwrjs/types": "0.13.3",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.6"
    },
    "optionalDependencies": {
        "esbuild": "^0.9.7"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "volta": {
        "extends": "../../../package.json"
    },
    "gitHead": "adaedf286df20aed374d9eb07eaf74668084073c"
}
